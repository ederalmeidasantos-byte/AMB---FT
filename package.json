{
  "name": "fgts-processor",
  "version": "4.7.0",
  "description": "Sistema de processamento automatizado de extratos FGTS",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop fgts-processor",
    "pm2:restart": "pm2 restart fgts-processor",
    "pm2:delete": "pm2 delete fgts-processor",
    "pm2:logs": "pm2 logs fgts-processor",
    "pm2:monit": "pm2 monit",
    "build": "echo 'No build step required'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fgts",
    "extrato",
    "processamento",
    "automacao"
  ],
  "author": "Sistema FGTS",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pm2": "^5.3.0",
    "qs": "^6.11.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
