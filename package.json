{
  "name": "clt-v8-api",
  "version": "1.0.0",
  "description": "API para integração CLT V8 Digital com Kentro",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node tests/teste-api.js",
    "test:auth": "node tests/testar-auth-v8.js",
    "test:fluxo": "node tests/teste-fluxo-clt.js",
    "test:simulacao": "node tests/testar-termo-clt.js",
    "logs": "tail -f logs/combined.log",
    "pm2:start": "pm2 start config/ecosystem.config.cjs",
    "pm2:stop": "pm2 stop clt-v8-api",
    "pm2:restart": "pm2 restart clt-v8-api",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs clt-v8-api",
    "test:precencabank-lote": "node scripts/testar-precencabank-lote-simples.js",
    "iniciar:precencabank-lote": "node scripts/iniciar-tudo-precencabank.js",
    "curriculo:pdf": "node gerar-pdf-curriculo.js",
    "setup": "npm install",
    "setup-e-iniciar": "npm install && npm start"
  },
  "keywords": [
    "clt",
    "v8",
    "api",
    "kentro",
    "integracao"
  ],
  "author": "Lunas Digital",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "puppeteer": "^24.29.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
